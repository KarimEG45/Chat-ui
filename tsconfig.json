{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "ES2018",
		"baseUrl": "./src",
		"paths": {
			"@components/*": ["components/*"],
			"@utils/*": ["utils/*"],
			"@pages/*": ["routes/*"]
		}
	}
}
